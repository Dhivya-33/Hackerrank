def average(arr):
    distinct_heights=set(arr)
    sum_distinct_heights=sum(distinct_heights)
    total_distinct_heights=len(distinct_heights)
    result=sum_distinct_heights/total_distinct_heights
    return result
    
    
    
if __name__ == '__main__':
    n = int(input())
    arr = list(map(int, input().split()))
    result = average(arr)
    print(result)
